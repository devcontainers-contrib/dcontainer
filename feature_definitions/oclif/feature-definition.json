{
    "id": "oclif",
    "version": "1.0.0",
    "name": "oclif generator (via npm)",
    "documentationURL": "http://github.com/devcontainers-contrib/features/tree/main/src/oclif",
    "description": "The oclif generator creates a CLI project in TypeScript to get you started quickly. It requires very few runtime dependencies and has extremely minimal overhead.",
    "options": {
        "version": {
            "default": "latest",
            "description": "Select the version to install.",
            "proposals": [
                "latest"
            ],
            "type": "string"
        }
    },
    "installsAfter": [
        "ghcr.io/devcontainers-contrib/features/npm-package"
    ],
    "dependencies": [
        {
            "feature": "ghcr.io/devcontainers-contrib/features/npm-package:1.0.2",
            "options": {
                "package": "oclif",
                "version": "$options.version"
            }
        }
    ],
    "install_command": "echo 'Done!'",
    "test_scenarios": [
        {
            "name": "test",
            "image": "mcr.microsoft.com/devcontainers/base:debian",
            "test_commands": [
                "oclif --version"
            ],
            "options": {}
        }
    ]
}
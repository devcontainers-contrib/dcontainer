{
    "id": "ory-kratos",
    "version": "1.0.1",
    "name": "Ory Kratos (via Github Releases)",
    "documentationURL": "http://github.com/devcontainers-contrib/features/tree/main/src/ory-kratos",
    "description": "Ory Kratos is the developer-friendly, security-hardened and battle-test Identity, User Management and Authentication system for the Cloud.",
    "options": {
        "version": {
            "default": "latest",
            "description": "Select the version to install.",
            "proposals": [
                "latest"
            ],
            "type": "string"
        }
    },
    "installsAfter": [
        "ghcr.io/devcontainers-contrib/features/gh-release"
    ],
    "dependencies": [
        {
            "feature": "ghcr.io/devcontainers-contrib/features/gh-release:1.0.17",
            "options": {
                "repo": "ory/kratos",
                "binaryNames": "kratos",
                "version": "$options.version",
                "assetRegex": ".*(sqlite).*"
            }
        }
    ],
    "install_command": "echo 'Done!'",
    "test_scenarios": [
        {
            "name": "test_defaults_debian",
            "image": "mcr.microsoft.com/devcontainers/base:debian",
            "test_commands": [
                "kratos version"
            ],
            "options": {}
        }
    ]
}
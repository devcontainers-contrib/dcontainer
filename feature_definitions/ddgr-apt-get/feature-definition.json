{
    "id": "ddgr-apt-get",
    "version": "1.0.12",
    "name": "ddgr (via apt-get)",
    "documentationURL": "http://github.com/devcontainers-contrib/features/tree/main/src/ddgr-apt-get",
    "description": "ddgr is a cmdline utility to search DuckDuckGo from the terminal.",
    "options": {},
    "installsAfter": [
        "ghcr.io/devcontainers-contrib/features/apt-get-packages"
    ],
    "dependencies": [
        {
            "feature": "ghcr.io/devcontainers-contrib/features/apt-get-packages:1.0.3",
            "options": {
                "packages": "ddgr"
            }
        }
    ],
    "install_command": "echo 'Done!'",
    "test_scenarios": [
        {
            "name": "test",
            "image": "mcr.microsoft.com/devcontainers/base:debian",
            "test_commands": [
                "ddgr --version"
            ],
            "options": {}
        }
    ]
}
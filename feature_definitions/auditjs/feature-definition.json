{
    "id": "auditjs",
    "version": "1.0.3",
    "name": "AuditJS (via npm)",
    "documentationURL": "http://github.com/devcontainers-contrib/features/tree/main/src/auditjs",
    "description": "AuditJS audits an NPM package.json file to identify known vulnerabilities.",
    "options": {
        "version": {
            "default": "latest",
            "description": "Select the version to install.",
            "proposals": [
                "latest"
            ],
            "type": "string"
        }
    },
    "installsAfter": [
        "ghcr.io/devcontainers/features/node",
        "ghcr.io/devcontainers-conrib/features/npm-package"
    ],
    "dependencies": [
        {
            "feature": "ghcr.io/devcontainers-contrib/features/npm-package:1",
            "options": {
                "package": "auditjs",
                "version": "$options.version"
            }
        }
    ],
    "install_command": "echo 'Done'",
    "test_scenarios": [
        {
            "name": "test_defaults",
            "image": "mcr.microsoft.com/devcontainers/base:debian",
            "test_commands": [
                "auditjs --version"
            ],
            "options": {},
            "features": {}
        }
    ]
}
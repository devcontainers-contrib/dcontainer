        {
            "feature": "ghcr.io/devcontainers-contrib/features/curl-apt-get:1.0.16",
            "options": {}
        }
    ],
    "install_command": "curl -fsSL https://tailscale.com/install.sh | sh",
    "test_scenarios": [
        {
            "name": "test",
            "image": "mcr.microsoft.com/devcontainers/base:debian",
            "test_commands": [
                "tailscale --version"
            ],
            "options": {}
        }
    ]
}
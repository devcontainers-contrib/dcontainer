{
    "id": "nushell",
    "version": "1.0.2",
    "name": "Nushell (via Github Releases)",
    "documentationURL": "http://github.com/devcontainers-contrib/features/tree/main/src/nushell",
    "description": "Nushell is a shell with a focus on operating on structured data in the command line.",
    "options": {
        "version": {
            "default": "latest",
            "description": "Select the version to install.",
            "proposals": [
                "latest"
            ],
            "type": "string"
        }
    },
    "installsAfter": [
        "ghcr.io/devcontainers-contrib/features/gh-release"
    ],
    "dependencies": [
        {
            "feature": "ghcr.io/devcontainers-contrib/features/gh-release:1.0.13",
            "options": {
                "repo": "nushell/nushell",
                "binaryNames": "nu",
                "version": "$options.version"
            }
        }
    ],
    "install_command": "echo 'Done!'",
    "test_scenarios": [
        {
            "name": "test_defaults_debian",
            "image": "mcr.microsoft.com/devcontainers/base:debian",
            "test_commands": [
                "nu --version"
            ],
            "options": {}
        }
    ]
}
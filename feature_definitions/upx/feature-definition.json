{
    "id": "upx",
    "version": "1.0.11",
    "name": "UPX (via Github Releases)",
    "documentationURL": "http://github.com/devcontainers-contrib/features/tree/main/src/upx",
    "description": "UPX is a free, secure, portable, extendable, high-performance executable packer for several executable formats.",
    "options": {
        "version": {
            "default": "latest",
            "description": "Select the version to install.",
            "proposals": [
                "latest"
            ],
            "type": "string"
        }
    },
    "installsAfter": [
        "ghcr.io/devcontainers-contrib/features/gh-release"
    ],
    "dependencies": [
        {
            "feature": "ghcr.io/devcontainers-contrib/features/gh-release:1.0.17",
            "options": {
                "repo": "upx/upx",
                "binaryNames": "upx",
                "version": "$options.version"
            }
        }
    ],
    "install_command": "echo 'Done!'",
    "test_scenarios": [
        {
            "name": "test_defaults_debian",
            "image": "mcr.microsoft.com/devcontainers/base:debian",
            "test_commands": [
                "upx --version"
            ],
            "options": {}
        }
    ]
}
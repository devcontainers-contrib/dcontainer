{
    "id": "go-task",
    "version": "1.0.1",
    "name": "Task (via Github Releases)",
    "documentationURL": "http://github.com/devcontainers-contrib/features/tree/main/src/go-task",
    "description": "Task is a task runner / build tool that aims to be simpler and easier to use than, for example, GNU Make.",
    "options": {
        "version": {
            "default": "latest",
            "description": "Select the version to install.",
            "proposals": [
                "latest"
            ],
            "type": "string"
        }
    },
    "installsAfter": [
        "ghcr.io/devcontainers-contrib/features/gh-release"
    ],
    "dependencies": [
        {
            "feature": "ghcr.io/devcontainers-contrib/features/gh-release:1.0.16",
            "options": {
                "repo": "go-task/task",
                "binaryNames": "task",
                "version": "$options.version"
            }
        }
    ],
    "install_command": "echo 'Done!'",
    "test_scenarios": [
        {
            "name": "test_defaults_debian",
            "image": "mcr.microsoft.com/devcontainers/base:debian",
            "test_commands": [
                "task --version"
            ],
            "options": {}
        }
    ]
}
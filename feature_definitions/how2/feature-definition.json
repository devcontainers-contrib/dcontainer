{
    "id": "how2",
    "version": "1.0.0",
    "name": "how2 (via Github Releases)",
    "documentationURL": "http://github.com/devcontainers-contrib/features/tree/main/src/how2",
    "description": "how2 finds the simplest way to do something in a unix shell. It's like man, but you can query it in natural language. It uses a mix of AI code-completion and StackOverflow search.",
    "options": {
        "version": {
            "default": "latest",
            "description": "Select the version to install.",
            "proposals": [
                "latest"
            ],
            "type": "string"
        }
    },
    "installsAfter": [
        "ghcr.io/devcontainers-contrib/features/gh-release"
    ],
    "dependencies": [
        {
            "feature": "ghcr.io/devcontainers-contrib/features/gh-release:1.0.11",
            "options": {
                "repo": "santinic/how2",
                "binaryNames": "how2",
                "version": "$options.version"
            }
        }
    ],
    "install_command": "echo 'Done!'",
    "test_scenarios": [
        {
            "name": "test_defaults_debian",
            "image": "mcr.microsoft.com/devcontainers/base:debian",
            "test_commands": [
                "how2 --version"
            ],
            "options": {}
        }
    ]
}
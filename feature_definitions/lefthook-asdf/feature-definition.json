{
    "id": "lefthook-asdf",
    "version": "1.0.0",
    "name": "Lefthook (via asdf)",
    "documentationURL": "http://github.com/devcontainers-contrib/features/tree/main/src/lefthook-asdf",
    "description": "Lefthook is the fastest polyglot Git hooks manager out there.",
    "options": {
        "version": {
            "default": "latest",
            "description": "Select the version to install.",
            "proposals": [
                "latest"
            ],
            "type": "string"
        }
    },
    "installsAfter": [
        "ghcr.io/devcontainers-contrib/features/asdf-package"
    ],
    "dependencies": [
        {
            "feature": "ghcr.io/devcontainers-contrib/features/asdf-package:1.0.5",
            "options": {
                "plugin": "lefthook",
                "version": "$options.version"
            }
        }
    ],
    "install_command": "echo 'Done!'",
    "test_scenarios": [
        {
            "name": "test",
            "image": "mcr.microsoft.com/devcontainers/base:debian",
            "test_commands": [
                "lefthook --help"
            ],
            "options": {}
        }
    ]
}
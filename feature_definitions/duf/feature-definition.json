{
    "id": "duf",
    "version": "1.0.6",
    "name": "duf (Disk Usage/Free Utility) (via Github Releases)",
    "documentationURL": "http://github.com/devcontainers-contrib/features/tree/main/src/duf",
    "description": "duf is a better 'df' alternative.",
    "options": {
        "version": {
            "default": "latest",
            "description": "Select the version to install.",
            "proposals": [
                "latest"
            ],
            "type": "string"
        }
    },
    "installsAfter": [
        "ghcr.io/devcontainers-contrib/features/gh-release"
    ],
    "dependencies": [
        {
            "feature": "ghcr.io/devcontainers-contrib/features/gh-release:1.0.11",
            "options": {
                "repo": "muesli/duf",
                "binaryNames": "duf",
                "version": "$options.version"
            }
        }
    ],
    "install_command": "echo 'Done!'",
    "test_scenarios": [
        {
            "name": "test_defaults_debian",
            "image": "mcr.microsoft.com/devcontainers/base:debian",
            "test_commands": [
                "duf --version"
            ],
            "options": {}
        }
    ]
}
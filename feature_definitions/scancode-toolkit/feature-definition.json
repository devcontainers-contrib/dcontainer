{
    "name": "Scan (SAST) (via pipx)",
    "id": "scancode-toolkit",
    "version": "1.0.0",
    "description": "ScanCode scan code to detect packages and dependencies, licenses, copyrights and more.",
    "documentationURL": "http://github.com/devcontainers-contrib/features/tree/main/src/scancode-toolkit",
    "installsAfter": [
        "ghcr.io/devcontainers-contrib/features/pipx-package",
        "ghcr.io/devcontainers/features/python"
    ],
    "options": {
        "version": {
            "type": "string",
            "proposals": [
                "latest"
            ],
            "default": "latest",
            "description": "Select the version to install."
        }
    },
    "dependencies": [
        {
            "feature": "ghcr.io/devcontainers-contrib/features/pipx-package:latest",
            "options": {
                "package": "scancode-toolkit",
                "version": "$options.version"
            }
        }
    ],
    "install_command": "echo 'Done'",
    "test_scenarios": [
        {
            "name": "test_defaults",
            "image": "mcr.microsoft.com/devcontainers/base:debian",
            "test_commands": [
                "scancode --version"
            ],
            "options": {}
        }
    ]
}
{
    "id": "volta",
    "version": "1.0.4",
    "name": "Volta (via Github Releases)",
    "documentationURL": "http://github.com/devcontainers-contrib/features/tree/main/src/volta",
    "description": "Volta is a hassle-free way to manage your JavaScript command-line tools.",
    "options": {
        "version": {
            "default": "latest",
            "description": "Select the version to install.",
            "proposals": [
                "latest"
            ],
            "type": "string"
        }
    },
    "installsAfter": [
        "ghcr.io/devcontainers-contrib/features/gh-release"
    ],
    "dependencies": [
        {
            "feature": "ghcr.io/devcontainers-contrib/features/gh-release:1.0.21",
            "options": {
                "repo": "volta-cli/volta",
                "binaryNames": "volta",
                "version": "$options.version"
            }
        }
    ],
    "install_command": "echo 'Done!'",
    "test_scenarios": [
        {
            "name": "test_defaults_debian",
            "image": "mcr.microsoft.com/devcontainers/base:debian",
            "test_commands": [
                "volta --version"
            ],
            "options": {},
            "features": {}
        }
    ]
}
{
    "id": "rclone",
    "version": "1.0.13",
    "name": "Rclone (via Github Releases)",
    "documentationURL": "http://github.com/devcontainers-contrib/features/tree/main/src/rclone",
    "description": "Rclone ('rsync for cloud storage') is a command-line program to sync files and directories to and from different cloud storage providers.",
    "options": {
        "version": {
            "default": "latest",
            "description": "Select the version to install.",
            "proposals": [
                "latest"
            ],
            "type": "string"
        }
    },
    "installsAfter": [
        "ghcr.io/devcontainers-contrib/features/gh-release"
    ],
    "dependencies": [
        {
            "feature": "ghcr.io/devcontainers-contrib/features/gh-release:1.0.20",
            "options": {
                "repo": "rclone/rclone",
                "binaryNames": "rclone",
                "version": "$options.version"
            }
        }
    ],
    "install_command": "echo 'Done!'",
    "test_scenarios": [
        {
            "name": "test_defaults_debian",
            "image": "mcr.microsoft.com/devcontainers/base:debian",
            "test_commands": [
                "rclone --version"
            ],
            "options": {},
            "features": {}
        }
    ]
}
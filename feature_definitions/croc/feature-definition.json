{
    "id": "croc",
    "version": "1.0.1",
    "name": "croc (via Github Releases)",
    "documentationURL": "http://github.com/devcontainers-contrib/features/tree/main/src/croc",
    "description": "croc is a tool that allows any two computers to simply and securely transfer files and folders.",
    "options": {
        "version": {
            "default": "latest",
            "description": "Select the version to install.",
            "proposals": [
                "latest"
            ],
            "type": "string"
        }
    },
    "installsAfter": [
        "ghcr.io/devcontainers-contrib/features/gh-release"
    ],
    "dependencies": [
        {
            "feature": "ghcr.io/devcontainers-contrib/features/gh-release:1.0.16",
            "options": {
                "repo": "schollz/croc",
                "binaryNames": "croc",
                "version": "$options.version"
            }
        }
    ],
    "install_command": "echo 'Done!'",
    "test_scenarios": [
        {
            "name": "test_defaults_debian",
            "image": "mcr.microsoft.com/devcontainers/base:debian",
            "test_commands": [
                "croc --version"
            ],
            "options": {}
        }
    ]
}
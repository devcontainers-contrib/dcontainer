{
    "id": "ripgrep",
    "version": "1.0.10",
    "name": "ripgrep (via Github Releases)",
    "documentationURL": "http://github.com/devcontainers-contrib/features/tree/main/src/ripgrep",
    "description": "ripgrep recursively searches directories for a regex pattern while respecting your gitignore.",
    "options": {
        "version": {
            "default": "latest",
            "description": "Select the version to install.",
            "proposals": [
                "latest"
            ],
            "type": "string"
        }
    },
    "installsAfter": [
        "ghcr.io/devcontainers-contrib/features/gh-release"
    ],
    "dependencies": [
        {
            "feature": "ghcr.io/devcontainers-contrib/features/gh-release:1.0.16",
            "options": {
                "repo": "BurntSushi/ripgrep",
                "binaryNames": "rg",
                "version": "$options.version"
            }
        }
    ],
    "install_command": "echo 'Done!'",
    "test_scenarios": [
        {
            "name": "test_defaults_debian",
            "image": "mcr.microsoft.com/devcontainers/base:debian",
            "test_commands": [
                "rg --version"
            ],
            "options": {}
        }
    ]
}